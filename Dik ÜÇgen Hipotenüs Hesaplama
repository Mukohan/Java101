import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int a , b;
        double c ;
        Scanner input = new Scanner(System.in);
        System.out.print("Kısa Kenar:");
        a = input.nextInt();

        System.out.print("Uzun Kenar:");
        b = input.nextInt();

        c =Math.sqrt((a * a) + (b * b));


        System.out.print("Hipotenüs:" + c );
